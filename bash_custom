#Screen hack to rename current window to running process
if [[ $TERM == "screen" ]]
  then
    export PS1="\033k\033\\\\${debian_chroot:+($debian_chroot)}\u@\h:\w\$ "
  fi


#Gnome alias for portability
if [[ $DESKTOP_SESSION == "gnome" ]]
  then
    alias open="gnome-open"
  fi
  
alias fuck='wget -qO - rage.thewaffleshop.net' 
